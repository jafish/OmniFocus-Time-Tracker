property timeTrackingFile : "Time-Tracking.oo3"property timeTrackingFilePath : "/Users/jafish/Documents/OmniPresence/OmniOutliner"property ofTaskAndProjectLinkPrefix : "omnifocus:///task/"-- OmniOutliner Document Columns - set these if you have them in your documentproperty pStartColumnName : "Start Time"property pEndColumnName : "End Time"property pEstimateColumnName : "Estimated Time"property pDurationColumnName : "Time Spent"-- Get the name of the first task with an empty end date for a work sliceset runningTaskNotification to my getFirstRunningTask()display notification runningTaskNotification with title "Currently Working On"on getFirstRunningTask()	set wasTaskRunning to false	tell document named timeTrackingFile of application "OmniOutliner Pro"		set unfinishedTasks to rows where value of cell pEndColumnName is missing value and name is "Work Slice"		if unfinishedTasks is not {} then			-- Return the name of the task			return name of parent of item 1 in unfinishedTasks		else			-- Set message to "No running tasks"			return "No running tasks"		end if	end tellend getFirstRunningTask